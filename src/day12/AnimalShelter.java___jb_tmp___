package day12;

import java.util.LinkedList;
import java.util.Queue;

public class AnimalShelter {
    public static String shelterToString(Queue<Pet> input) {
        String output = "";
        for (Pet pet : input) {
            output = output + pet.name + " , ";
        }
        return output;
    }


